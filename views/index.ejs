<div class="container">
  <h1 class="text-2xl font-bold mb-4">Popular books</h1>
  <div class="-m-1 flex flex-wrap md:-m-2">
    <% books.forEach((book)=>{ %>
    <a
      class="w-1/3 md:w-1/4 lg:w-1/6 flex-wrap mx-1"
      href="/">
      <img
        height="150"
        width="100"
        class="max-w-full mx-auto rounded-lg mb-2 h-40 w-32 md:h-60 md:w-48"
        src="<%= book.coverImagePath  %>"
        alt="" />

      <h2 class="text-lg font-semibold text-center">Image 1 Title</h2>
    </a>
    <% }) %>
  </div>
</div>
