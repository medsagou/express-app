<h2>Search Book</h2>
<form
  action="/books"
  method="GET">
  <div>
    <label for="title">Title</label>
    <input
      type="text"
      name="title"
      value="<%= searchOptions.title %>" />
  </div>
  <div>
    <label for="publishedAfter">Published After</label>
    <input
      type="number"
      name="publishedAfter"
      value="<%= searchOptions.publishedAfter %>"
      max="<%= new Date().getFullYear() %>" />
  </div>
  <div>
    <label for="publishedBefore">Published Before</label>
    <input
      type="number"
      name="publishedBefore"
      value="<%= searchOptions.publishedBefore %>"
      max="<%= new Date().getFullYear() %>" />
  </div>
  <button type="submit">Search</button>
</form>
<div>
  <% books.forEach((book)=>{ %>
  <a href="/">
    <img
      height="150"
      width="100"
      src="<%= book.coverImagePath  %>"
      alt="" />
  </a>
  <% }) %>
</div>
